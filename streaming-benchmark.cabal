cabal-version:      2.4
name:               streaming-benchmark
version:            0

executable tmp-streaming
    main-is:          Main.hs
    build-depends:    base,
                      clock,
                      streaming == 0.2.3.0,
                      temporary,
                      transformers,
                      streamly == 0.8.0,
                      pipes == 4.3.16,
                      conduit == 1.3.4.2,
                      kan-extensions
    hs-source-dirs:   app
    other-modules:    Benchmark, Tree, Streaming.Bind, Streaming.Codensity
    default-language: Haskell2010
    ghc-options:      -Wall -O2
